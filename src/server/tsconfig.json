{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true,
    "outDir": "../../dist/server"
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "../../node_modules",
    "../../dist"
  ]
}