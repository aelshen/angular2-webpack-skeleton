{
  "name": "angular2-webpack-skeleton",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean:dist --parallel build:*",
    "build:client": "webpack",
    "build:server": "tsc -p src/server",
    "clean": "npm-run-all --parallel clean:*",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "nodemon": "nodemon --debug --delay 5 dist/server",
    "start": "node dist/server",
    "start:dev": "npm-run-all build:server --parallel nodemon webpack-dev-server",
    "test:client": "npm-run-all test:client:*",
    "test:client:unit": "karma start",
    "webpack": "webpack --config config/webpack.prod.js --progress --display-error-details",
    "webpack-dev-server": "webpack-dev-server -d --inline --host 0.0.0.0 --port 8080"
  },
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "cors": "^2.8.1",
    "express": "^4.15.2",
    "http": "0.0.0",
    "nconf": "^0.8.4",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.2.0",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.0.32",
    "@types/jasmine": "2.5.38",
    "@types/nconf": "^0.0.33",
    "@types/node": "^6.0.39",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.5.2",
